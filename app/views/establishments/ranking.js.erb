<% if !@places.empty? %>
  $('#rankingListAll').html("<%= escape_javascript render 'ranking_list' %>");
  $('#share').html("<%= escape_javascript render 'share' %>")
  $('#showMoreRanking').show();
  $('.filter').show();
  $('#share-buttons').show();
<% else %>
  $('#rankingListAll').html("<%= escape_javascript render 'nothing_found' %>");
  $('#showMoreRanking').hide();
  $('.filter').hide();
  $('#share-buttons').hide();
<% end %>
